import java.util.*;
class Main 
{
    public static void main(String[] args)
    {
        Scanner get=new Scanner(System.in);
        int n=get.nextInt();
        int a[]=new int[n];
        for(int i=0;i<n;i++)
        {
            a[i]=get.nextInt();
        }
        for(int i=0;i<n;i+=2)
        {
            System.out.print(a[i]+" ");
        }
    }
}
